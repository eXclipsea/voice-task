{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "VoiceTask",
  "version": "0.1.0",
  "identifier": "com.eclipsea.voicetask",
  "build": {
    "frontendDist": "https://voicetask-phi.vercel.app",
    "devUrl": "http://localhost:3004"
  },
  "app": {
    "windows": [
      {
        "title": "VoiceTask",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
